<?xml version="1.0" encoding="UTF-8"?>
<graph-specs output="barabasi_1k_5k_2el.fgf">
	<model name="barabasi">
		<n>1000</n>
		<m>5</m>
	</model>
	<edges>
		<labels>
			<label>friend</label>
			<label>family</label>
		</labels>
		<properties>
			<property name="time" type="integer">
				<min>0</min>
				<max>10000</max>
				<distribution name="gaussian" />
			</property>
		</properties>
	</edges>
	<vertices>
		<properties>
			<property name="age" type="integer">
				<min>18</min>
				<max>65</max>
				<distribution name="uniform" />
			</property>
		</properties>
	</vertices>
</graph-specs>

